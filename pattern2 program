import java.util.*;
class Pattern2
{
  public static void main(String args[])
  {
    int i,j,k,l;
    for(i=1;i<=5;i++)
    {
      for(j=1;j<=i;j++)
      {
        System.out.print("*");
      }
      System.out.print("\n");
    }
    for(k=5;k>=1;k--)
    {
      for(l=k;l>=1;l--)
      {
        System.out.print("*");
      }
      System.out.print("\n");
    }
  }
}
